    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "^9.0.0",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "_moduleAliases": {
    "@core": "dist/core",
    "@utilities": "dist/utilities",
    "@base": "dist",
    "@config": "dist/config",
    "@models": "dist/models",
    "@services": "dist/services",
    "@error-handling": "dist/error-types",
    "@policies": "dist/policies",
    "@guards": "dist/guards"
  }
