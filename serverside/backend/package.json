{
  "name": "Smurfjs",
  "version": "1.0.0",
  "description": "Microservices for node.js",
  "main": "index.js",
  "mainDir": "dist",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc",
    "dev:start": "npm-run-all clean build start",
    "start": "pm2-runtime start ./process.config.js;",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm": "node --require ts-node/register -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "dev": "./node_modules/.bin/tsc-watch --onSuccess \"npm run start\"",
    "smurf": "plop --plopfile src/core/generator/index.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bull": "^3.15.0",
    "@types/cors": "^2.8.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.28",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "bull": "^3.20.1",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-validator": "^6.6.1",
    "express-winston": "^4.0.5",
    "glob": "^7.1.6",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mysql": "^2.18.1",
    "nodemailer": "^6.4.14",
    "pg": "^8.5.1",
    "redis": "^3.0.2",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.21",
    "validatorjs": "^3.22.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/csvtojson": "^1.1.5",
    "@types/express": "^4.17.8",
    "@types/express-fileupload": "^1.1.6",
    "@types/fs-extra": "^9.0.2",
    "@types/lodash": "^4.14.162",
    "@types/node": "^14.11.8",
    "@types/shelljs": "^0.8.8",
    "@types/validator": "^13.1.3",
    "@types/validatorjs": "^3.15.0",
    "fs-extra": "^9.0.1",
